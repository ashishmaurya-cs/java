<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
 "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
 <beans>
 <bean id="id1" class="com.ashish.Address">
 <property name="addressline" value="AyreGaon"></property>
 <property name="city" value="Dombivli"></property>
 <property name="state" value="MH"></property>
 </bean>
 
 <bean id="id2" class="com.ashish.Employee">
 <property name="id" value="10"></property>
 <property name="name" value="Ashish"></property>
 <property name="address" ref="id1"></property>
 </bean>
 </beans>