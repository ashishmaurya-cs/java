<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
 "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
 <beans>
 <bean id="id1" class="com.ashish.Answer">
 <property name="answer" value="Mysql is a database"></property>
 <property name="by" value="Ashish"></property>
 </bean>
 
  <bean id="id2" class="com.ashish.Answer">
 <property name="answer" value="Mysql is a RDBMS"></property>
 <property name="by" value="Ajeet"></property>
 </bean>
 
 <bean id="id3" class="com.ashish.Question">
 <property name="id" value="13"></property>
 <property name="question" value="What is Mysql"></property>
 <property name="answer">
 <list>
 <ref bean="id1"/>
 <ref bean="id2"/>
 </list>
 </property>
 </bean>
 
 </beans>