<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
 "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
 <beans>
 <bean id="id1" class="com.ashish.AnswerSet">
 <property name="id" value="10"></property>
 <property name="answers" value="java is language"></property>
 <property name="by" value="Ashish"></property>
 </bean>
 
  <bean id="id2" class="com.ashish.AnswerSet">
 <property name="id" value="10"></property>
 <property name="answers" value="java is language"></property>
 <property name="by" value="Ashish"></property>
 </bean>
 
 <bean id="id3" class="com.ashish.QuestionSetObject">
 <property name="id" value="1"></property>
 <property name="question" value="What is java"></property>
 <property name="answers">
 <set>
 <ref bean="id1"/>
 <ref bean="id2"/>
 </set>
 </property>
 </bean>
 </beans>