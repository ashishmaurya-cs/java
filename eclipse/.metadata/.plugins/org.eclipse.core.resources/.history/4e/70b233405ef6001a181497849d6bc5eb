package com.ashish;

import java.util.Scanner;

public class Pattern1 {
	public static void main(String[] args) {
		System.out.println("Enter number of row nad column :-");
		Scanner sc= new Scanner(System.in);
		int m=sc.nextInt();
		int n=sc.nextInt();
		for(int i=1;i<=m;i++){
			for(int j=1;j<=n;j++){
				if(j<=i){
					System.out.print("* ");
				}
				else{
					System.out.print("");
				}
			}
			System.out.println();
		}
		sc.close();
	}
}
